                            "clusterName": "ext-authz-pdp"
                          }
                        },
                        "transportApiVersion": "V3",
                        "withRequestBody": {
                          "maxRequestBytes": 8192,
                          "allowPartialMessage": true,
                          "packAsBytes": true
                        },
                        "includePeerCertificate": true
                      }
